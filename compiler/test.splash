
main {
    var name = "Yaniv"

    var p = Person("Yaniv",-2)
    p.sayHi()

    print("Person's name is {p.name}")

    print("Is {p.name}'s age odd? {p.age.isOdd()}")
}

class Person {
    string name
    int age

    constructor(this.name, int? age) {
        this.age = age ~ 5
    }

    sayHi() {
        print("{name} says hi, he's {age} years old")
    }
}